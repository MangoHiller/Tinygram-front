ACT,WAAW,CAACO,KAAK,CAAtC,IAAsC,CAAN,CAAZ,CAApB;AAEA,SAAOD,QAAQ,CAAQ;AACrBI,IAAAA,GAAG,GAAI;AACL,UAAIL,aAAa,CAAjB,KAAA,EAAyB,OAAOL,WAAW,CAACO,KAAK,CAAjD,IAAiD,CAAN,CAAlB,CAAzB,KACK,OAAOC,QAAQ,CAAf,KAAA;AAHc,KAAA;;AAKrBG,IAAAA,GAAG,CAAA,QAAA,EAAY;AACbH,MAAAA,QAAQ,CAARA,KAAAA,GAAAA,QAAAA;AACAL,MAAAA,EAAE,IAAFA,IAAAA,GAAAA,KAAAA,CAAAA,GAAAA,EAAE,CAAFA,IAAAA,CAAU,UAASS,IAAnBT,EAAAA,EAA2BD,YAAY,CAAvCC,QAAuC,CAAvCA,CAAAA;AACD;;AARoB,GAAR,CAAf;AAUD","sourcesContent":["// Utilities\nimport { computed, getCurrentInstance, ref } from 'vue'\nimport { consoleError, toKebabCase } from '@/util'\n\n// Types\nimport type { Ref } from 'vue'\n\n// Composables\nexport function useProxiedModel<\n  Props extends object,\n  Prop extends Extract<keyof Props, string>,\n  Inner = Props[Prop],\n> (\n  props: Props,\n  prop: Prop,\n  defaultValue?: Props[Prop],\n  transformIn: (value?: Props[Prop]) => Inner = (v: any) => v,\n  transformOut: (value: Inner) => Props[Prop] = (v: any) => v,\n) {\n  const vm = getCurrentInstance()\n\n  if (!vm) consoleError('useProxiedModel must be called from inside a setup function')\n\n  const propIsDefined = computed(() => {\n    return !!(\n      typeof props[prop] !== 'undefined' &&\n      (vm?.vnode.props?.hasOwnProperty(prop) || vm?.vnode.props?.hasOwnProperty(toKebabCase(prop)))\n    )\n  })\n\n  const internal = ref(transformIn(props[prop])) as Ref<Inner>\n\n  return computed<Inner>({\n    get () {\n      if (propIsDefined.value) return transformIn(props[prop])\n      else return internal.value\n    },\n    set (newValue) {\n      internal.value = newValue\n      vm?.emit(`update:${prop}`, transformOut(newValue))\n    },\n  })\n}\n"]}]}