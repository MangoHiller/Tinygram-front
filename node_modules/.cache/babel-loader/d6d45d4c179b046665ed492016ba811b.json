rray.isArray(vnode)) {\n    return vnode\n      .map(child => findChildren(child))\n      .filter(v => v)\n      .flat(1)\n  } else if (Array.isArray(vnode.children)) {\n    return vnode.children\n      .map(child => findChildren(child))\n      .filter(v => v)\n      .flat(1)\n  } else if (vnode.component) {\n    return [vnode.component, ...findChildren(vnode.component?.subTree)]\n      .filter(v => v)\n      .flat(1)\n  }\n\n  return []\n}\n"]}]}